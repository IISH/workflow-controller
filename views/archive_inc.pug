each archive in archives || []
    tr(class='small')
        td
            a(href='/history?form_archive_name=' + archive.archive) #{archive.archive}
        td #{archive.complete || 0}
        td #{archive.running || 0}
        td #{archive.waiting || 0}
        td #{archive.failed || 0}
        td #{archive.total || 0}
        td
            a(href='archive/delete/' + archive.archive class='small delete_row' role='button' onclick='return confirm("Are you sure you want to delete?");') delete